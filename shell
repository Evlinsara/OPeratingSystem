Program 1

echo "Enter your name:"
read name
echo "Enter your batch:"
read batch
echo "Enter your rollno:"
read roll
echo "Name:"$name
echo "Batch:"$batch
echo "Roll no:"$roll

Program 2

echo "Enter first no:"
read a
echo "Enter second no:"
read b
if (($a>$b))
then
echo "$a is greater than $b"
else
echo "$b is greater than $a"
fi

Program 3

echo "Enter limit:"
read n
echo "Natural numbers upto $n:"
for((i=1;i<=$n;i++))
do
echo $i
done

Program 4

echo "Enter number:"
read a
fact=1
for((i=1;i<$a;i++))
do
fact=$(($fact*$i))
done
echo "Factorial is $fact"

Program 5

echo "Enter limit:"
read n
a=0
b=1
echo "Fibonacci series upto $n is"
echo "$a"
echo "$b"
while(($b<=$n))
do
next=$((a+b))
echo "$next"
a=$b
b=$next
done
